require 'rubygems'
require 'sprout'
require 'sprout/air/version'

exe = Sprout::Sprout.get_executable('sprout-air-tool', 'bin/adl', Sprout::AIR::VERSION::STRING)
exec("#{exe} #{ARGV.join(' ')}")
