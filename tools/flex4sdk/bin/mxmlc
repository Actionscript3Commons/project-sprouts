require 'rubygems'
require 'sprout'
require 'sprout/flex4sdk/version'

exe = Sprout::Sprout.get_executable('sprout-flex4sdk-tool', 'bin/mxmlc', Sprout::Flex4SDK::VERSION::STRING)
exec("#{exe} #{ARGV.join(' ')}")
