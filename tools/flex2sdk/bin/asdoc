require 'rubygems'
require 'sprout'
require 'sprout/flex2sdk/version'

exe = Sprout::Sprout.get_executable('sprout-flex2sdk-tool', 'bin/asdoc', Sprout::Flex2SDK::VERSION::STRING)
exec("#{exe} #{ARGV.join(' ')}")
