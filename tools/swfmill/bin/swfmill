require 'rubygems'
require 'sprout'
require 'sprout/swfmill/version'

exe = Sprout::Sprout.get_executable('sprout-swfmill-tool', nil, Sprout::SWFMill::VERSION::STRING)
exec("#{exe} #{ARGV.join(' ')}")
