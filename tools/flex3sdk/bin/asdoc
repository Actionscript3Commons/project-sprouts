require 'rubygems'
require 'sprout'
require 'sprout/flex3sdk/version'

exe = Sprout::Sprout.get_executable('sprout-flex3sdk-tool', 'bin/asdoc', Sprout::Flex3SDK::VERSION::STRING)
exec("#{exe} #{ARGV.join(' ')}")
